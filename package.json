{
  "name": "@supabase-community/vercel-ai-chatbot",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' concurrently \"npm run dev:partykit\" \"npm run dev:next\" --kill-others",
    "dev:next": "next dev",
    "dev:partykit": "partykit dev",
    "start": "next dev",
    "build": "next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "create:migration": "npx supabase migration new",
    "generate:migration": "npx supabase db diff -f $1",
    "stripe:listen": "stripe listen --forward-to=localhost:3000/api/webhooks",
    "generate:types:prod": "npx supabase gen types typescript  --schema public > ./lib/db_types.ts --project-id",
    "generate:types:local": "npx supabase gen types typescript --local --schema public > ./lib/db_types.ts",
    "prepare": "husky install",
    "type-check": "tsc --noEmit",
    "format:write": "prettier --write \"{app,lib,components}/**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"{app,lib,components}**/*.{ts,tsx,mdx}\" --cache"
  },
  "engines": {
    "node": ">=18.x.x"
  },
